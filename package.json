{
  "name": "schulcloud-content-crawler",
  "version": "0.0.0",
  "description": "Service to gather content from various education sites and providing a search",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "linter": "eslint src",
    "test": "mocha"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schulcloud/schulcloud-content-crawler"
  },
  "author": {},
  "license": "MIT",
  "dependencies": {
    "async-q": "^0.3.1",
    "body-parser": "^1.13.3",
    "bottleneck": "^1.15.0",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "crypto": "0.0.3",
    "express": "^4.13.3",
    "libxmljs": "^0.18.0",
    "lodash": "^4.17.2",
    "md5": "^2.2.1",
    "moment-timezone": "^0.5.11",
    "mongoose": "^4.7.5",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "resource-router-middleware": "^0.5.1",
    "striptags": "^2.1.1",
    "url-join": "^1.1.0",
    "xml2js-es6-promise": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "sinon-as-promised": "^4.0.2"
  }
}
